#include<Servo.h>
Servo servo1;
Servo servo2;
int const s1 = 4;
int const s2 = 3;

int const enA =6;
int const in1 =8;
int const in2 =7;

int pos = 0;
void setup()
{
  servo1.attach(s1);
  servo2.attach(s2);
  
  pinMode(enA, OUTPUT);
  pinMode(in1, OUTPUT);
  pinMode(in2, OUTPUT);
}

void loop() {

  servo1.write(180); 
  servo2.write(180); 
  delay(100);


  digitalWrite(in1, HIGH);
  digitalWrite(in2, LOW);
  analogWrite(enA, 200); 
  delay(100);


  digitalWrite(in1, LOW);
  digitalWrite(in2, LOW);
  

  servo1.write(90);
  servo2.write(0);
  delay(100);
}
